ALGORITHM algorithm_name
VAR
    arr:array_name : ARRAY_OF INTEGER[9];
    i,j,temp : INTEGER
BEGIN
    FOR i FROM 0 TO to array lenght-1 STEP 1 DO
    FOR j : i+1 and j>0 STEP-1 DO
    IF (arr j < arr[j-1]) THEN
    temp := arr<[j]
    arr[j]:arr[j-1]
    arr[j-1]:temp
   
    END_IF
    
    END_FOR
END